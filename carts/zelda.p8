pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
 palt(0,false)
 palt(14,true)
 poke(0x5f2e,1)
 pal(11, 11+128,1)
 pal(2, 3+128,1)
 
 px=64
 py=64
 pspr=0
end

function _draw()
 map()
 spr(pspr,px,py,2,2)
end

function _update60()
 newx=px
 newy=py
 if btnp(⬅️) then
  newx=px-16
  pspr=4
 end
 if btnp(➡️) then
  newx=px+16
  pspr=8
 end
 if btnp(⬆️) then
  newy=py-16
  pspr=12
 end
 if btnp(⬇️) then
  newy=py+16
  pspr=0
 end
 
 if walkable(newx,newy) then
  px=newx
  py=newy
 end
end


function walkable(pnx, pny)
 mytile=mget(pnx/8, pny/8)
 if not fget(mytile, 0) then
  return true
 end
 return false

end
